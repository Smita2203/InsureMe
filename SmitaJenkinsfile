
pipeline {
  agent any

  tools {
    maven 'M2_HOME'
    }
  
  stages {
    stage('Checkout') {
      steps {
        echo 'Checkout the source code from GitHub'
        git branch: 'main', url: 'https://github.com/Smita2203/InsureMe.git'
            }
    }
   stage('Package') {
      steps {
        echo 'Build code'
        sh 'mvn clean package'
            }
    }
}
}
